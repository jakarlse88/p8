    </Content>
    <Content Update="Pages\Subcomponents\ServicesOverview.razor">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
    </Content>
    <Content Update="Pages\Index\Subcomponents\ContactForm.razor">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
    </Content>
    <Content Update="Pages\Booking\Booking.razor">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
    </Content>
    <Content Update="Shared\Banner.razor">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
    </Content>
    <Content Update="Pages\ConsultantOverview\ConsultantOverview.razor">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
    </Content>
